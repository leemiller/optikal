<select class="tonic">
    {{#each notes}}
    <option value="{{this}}">{{this}}</option>
    {{/each}}
</select>

<select class="scale">
    {{#each scales}}
    <option value="{{this}}">{{this}}</option>
    {{/each}}
</select>

<select class="instrument">
    {{#each instruments}}
    <option value="{{this}}">{{this}}</option>
    {{/each}}
</select>